{
  "name": "ECG",
  "version": "0.0.1",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "bootstrap": "yarn && yarn bootstrap:pods",
    "bootstrap:pods": "bundle install && cd ios && bundle exec pod install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint-fix": "eslint --fix . ",
    "start": "react-native webpack-start --resetCache --port 9004",
    "start:standalone": "STANDALONE=1 react-native webpack-start --resetCache",
    "bundle:ios": "react-native webpack-bundle --platform ios --dev=false --entry-file index.js",
    "bundle:android": "react-native webpack-bundle --platform android  --dev=false --entry-file index.js",
    "bundle": "yarn bundle:ios && yarn bundle:android",
    "test": "jest"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.3",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/cli-platform-android": "^13.2.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.5",
    "@shopify/restyle": "^2.4.2",
    "axios": "^1.6.7",
    "react": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-native": "0.72.5",
    "react-native-click-outside": "^0.1.1",
    "react-native-device-info": "^10.12.0",
    "react-native-gesture-handler": "^2.13.1",
    "react-native-modal": "^13.0.1",
    "react-native-outside-press": "^1.2.2",
    "react-native-reanimated": "^3.6.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "^14.1.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-web": "^0.19.10",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@callstack/repack": "^3.7.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "4.8.4",
    "webpack": "^5.90.0"
  },
  "engines": {
    "node": ">=18"
  }
}
